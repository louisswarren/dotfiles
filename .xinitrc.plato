#!/bin/sh

if [ -d /etc/X11/xinit/xinitrc.d ] ; then
 for f in /etc/X11/xinit/xinitrc.d/?*.sh ; do
  [ -x "$f" ] && . "$f"
 done
 unset f
fi

./.fehbg &
./bin/msens "Telink Wireless Receiver Mouse" -0.2 &
udiskie &
xbindkeys &
xmodmap ~/.Xmodmap &
xset -b &

./bin/xrdbbase16 gruvbox-light-soft

exec i3
