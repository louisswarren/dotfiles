#!/bin/sh

if [ -d /etc/X11/xinit/xinitrc.d ] ; then
 for f in /etc/X11/xinit/xinitrc.d/?*.sh ; do
  [ -x "$f" ] && . "$f"
 done
 unset f
fi

./.fehbg &
./bin/msens "USB OPTICAL MOUSE" 0.45 &
numlockx &
udiskie &
xbindkeys &
xmodmap ~/.Xmodmap &
xset -b &

# gruvbox-dark-pale
./bin/xrdbbase16
xrdb -merge ~/.Xresources

exec i3
