#!/bin/sh

if [ -d /etc/X11/xinit/xinitrc.d ] ; then
 for f in /etc/X11/xinit/xinitrc.d/?*.sh ; do
  [ -x "$f" ] && . "$f"
 done
 unset f
fi

./.fehbg &
./bin/clipslate &
xinput set-prop "USB OPTICAL MOUSE " "libinput Accel Speed" -0.55
numlockx &
udiskie &
xbindkeys &
xmodmap ~/.Xmodmap &
xset -b &

./bin/xrdbbase16 gruvbox-dark-pale

exec i3
